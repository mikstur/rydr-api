{
  "name": "AdventureFeed",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "adventureFeedId": {
      "type": "string",
      "id": true,
      "required": true,
      "defaultFn": "uuid"
    },
    "adventureId": {
      "type": "string",
      "required": true
    },
    "userId": {
      "type": "number"
    },
    "action": {
      "type": "string"
    },
    "createdOn": {
      "type": "date",
      "defaultFn": "now"
    },
    "updateOn": {
      "type": "date",
      "defaultFn": "now"  
    }
  },
  "validations": [],
  "relations": {
    "adventure": {
      "type": "belongsTo",
      "model": "Adventure",
      "foreignKey": "adventureId"
    },  
    "user": {
      "type": "belongsTo",
      "model": "RbgUser",
      "foreignKey": "userId"
    }
  },
  "acls": [],
  "methods": {}
}

